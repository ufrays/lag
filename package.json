    "cds-mysql": "^6",
    "express": "^4",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@sap/cds-dk": "6.1.5",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/ux-specification": "^1.102.11",
    "@sapui5/ts-types": "^1.92.2",
    "@types/express": "^4.17.14",
