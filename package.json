    "jest": "^29.0.3",
    "jest-junit": "^14.0.1",
    "node-notifier": "^10.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "CDS_ENV=development cds run",
    "start:p": "CDS_ENV=production cds run",
    "build": "cds build && npm run defGen",
    "build:ts": "tsc -p tsconfig.build.json",
    "defGen": "cds2types --cds ./db ./srv -o ./srv/utils/schemaGen.ts --prefix I -f",
    "watch-user-monitoring": "cds watch --open user-monitoring/webapp/index.html#fe-lrop-v4",
    "watch-user-management": "cds watch --open user-management/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-activity-managment": "cds watch --open activity-managment/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-ship-management": "cds watch --open ship-management/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-user-ui": "cds watch --open user-ui/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-lag-ui": "cds watch --open lag-ui/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "[development]": {
        "db": {
          "kind": "sql",
          "credentials": {
            "database": "lag.db"
          }
        }
      },
      "[production]": {
        "db": {
          "kind": "mysql",
          "tenant": {
            "deploy": {
              "eager": [
                "default"
              ]
            }
          },
          "credentials": {
            "host": "139.196.90.54",
            "user": "Dev",
            "password": "Welcome1",
            "database": "lag",
            "port": 3306
          }
        },
        "mysql": {
          "impl": "cds-mysql"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/user-monitoring",
    "app/user-monitoring",
    "app/user-management",
    "app/activity-managment",
    "app/ship-management",
    "app/user-ui",
    "app/user-management",
    "app/project1",
    "app/project1",
    "app/lag-ui"
  ]
}